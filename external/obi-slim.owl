        <obo:IAO_0000114 rdf:resource="http://purl.obolibrary.org/obo/IAO_0000123"/>
        <obo:IAO_0000115>An assay that measures the occurrence of death events in one or more organisms over time</obo:IAO_0000115>
        <rdfs:label>survival assessment assay</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/OBI_0000705 -->


    <owl:Class rdf:about="http://purl.obolibrary.org/obo/OBI_0000705">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/OBI_0600047"/>
        <obo:IAO_0000111>Edman degradation</obo:IAO_0000111>

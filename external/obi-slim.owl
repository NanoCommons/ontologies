    <!-- http://purl.obolibrary.org/obo/OBI_0002175 -->


    <owl:Class rdf:about="http://purl.obolibrary.org/obo/OBI_0002175">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/OBI_0000443"/>
